/*
x^4/4 + 1 = (x^2/2 - x + 1) * (x^2/2 + x + 1)

Let x = 2^((10000000002+2)/4) =>
N = 2^10000000002 + 1 = x^4/4 + 1
*/


{
M = 10^10;
x = Mod(2, M)^((10000000002+2)/4);
xx = Mod(2, 2*M)^((10000000002+2)/4);
f1 = Mod(lift(xx^2)/2, M) - x + 1;
f2 = Mod(lift(xx^2)/2, M) + x + 1;
printf("%d,%d\n", lift(f1), lift(f2));
}
