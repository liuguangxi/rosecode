/*
y = 4*x^2 / (1 + 4*x^2) =>
z = 4*y^2 / (1 + 4*y^2) = 64*x^4/(80*x^4 + 8*x^2 + 1)
x = 4*z^2 / (1 + 4*z^2) = 16384*x^8/(22784*x^8 + 1280*x^6 + 224*x^4 + 16*x^2 + 1)
=>
22784*x^8 - 16384*x^7 + 1280*x^6 + 224*x^4 + 16*x^2 + 1 = 0
=>
rational number x = 1/2
*/


{
x = 1/2;
y = 4*x^2 / (1 + 4*x^2);
z = 4*y^2 / (1 + 4*y^2);
print(x, ",", y, ",", z);
}
